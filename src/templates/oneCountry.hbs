{{#each this}} <li class="list-item">
    <div class="flag">
        <img src="{{flag}}" alt="flag">
    </div>
    <div class="description">
        <h2>{{name}}</h2>
        <p>Capital: {{capital}}</p>
        <p>Population: {{population}}</p>
        <p>Languages: {{#each languages}}{{name}}</p>{{/each}}
    </div>
</li>
{{/each}}